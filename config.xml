<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.telegraf.money" version="2.15.10" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
  <name>Telegraf Money</name>
  <description>Telegraf Money: Messenger  allows members to communicate with each other, instantly send funds and
        issue/receive loans online.
    </description>
  <author email="developers@telegraf.money" href="https://telegraf.money/">Telegraf Money LLC.</author>
  <license href="https://telegraf.money/license">MIT</license>
  <content src="index.html"/>
  <meta http-equiv="Content-Security-Policy" content="default-src 'self' data: gap: ms-appdata: https://ssl.gstatic.com 'unsafe-eval'; style-src 'self' 'unsafe-inline'; media-src *"/>
  <preference name='phonegap-version' value='cli-7.0.1' />
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="android-minSdkVersion" value="16"/>
  <preference name="AndroidPersistentFileLocation" value="Compatibility"/>

  <preference name='pgb-builder-version' value='1' />
  <gap:config-file platform="ios" parent="NSCameraUsageDescription" overwrite="true">
    <string>We are using the Camera for something..</string>
  </gap:config-file>
  <icon src="icon.png"/>
  <icon height="29" platform="ios" src="res/icons/ios/icon-small.png" width="29"/>
  <icon height="58" platform="ios" src="res/icons/ios/icon-small-2x.png" width="58"/>
  <icon height="40" platform="ios" src="res/icons/ios/icon-40.png" width="40"/>
  <icon height="80" platform="ios" src="res/icons/ios/icon-40-2x.png" width="80"/>
  <icon height="50" platform="ios" src="res/icons/ios/icon-50.png" width="50"/>
  <icon height="100" platform="ios" src="res/icons/ios/icon-50-2x.png" width="100"/>
  <icon height="57" platform="ios" src="res/icons/ios/icon.png" width="57"/>
  <icon height="114" platform="ios" src="res/icons/ios/icon-2x.png" width="114"/>
  <icon height="60" platform="ios" src="res/icons/ios/icon-60.png" width="60"/>
  <icon height="120" platform="ios" src="res/icons/ios/icon-60-2x.png" width="120"/>
  <icon height="180" platform="ios" src="res/icons/ios/icon-60-3x.png" width="180"/>
  <icon height="72" platform="ios" src="res/icons/ios/icon-72.png" width="72"/>
  <icon height="144" platform="ios" src="res/icons/ios/icon-72-2x.png" width="144"/>
  <icon height="76" platform="ios" src="res/icons/ios/icon-76.png" width="76"/>
  <icon height="152" platform="ios" src="res/icons/ios/icon-76-2x.png" width="152"/>
  <icon platform="android" qualifier="ldpi" src="res/icons/android/icon-36-ldpi.png"/>
  <icon platform="android" qualifier="mdpi" src="res/icons/android/icon-48-mdpi.png"/>
  <icon platform="android" qualifier="hdpi" src="res/icons/android/icon-72-hdpi.png"/>
  <icon platform="android" qualifier="xhdpi" src="res/icons/android/icon-96-xhdpi.png"/>
  <icon platform="android" qualifier="xxhdpi" src="res/icons/android/icon-144-xxhdpi.png"/>
  <icon platform="android" qualifier="xxxhdpi" src="res/icons/android/icon-192-xxxhdpi.png"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="6000" />

  <splash src="res/screens/android/screen-ldpi-portrait.png" platform="android" qualifier="ldpi" />
  <splash src="res/screens/android/screen-mdpi-portrait.png" platform="android" qualifier="mdpi" />
  <splash src="res/screens/android/screen-hdpi-portrait.png" platform="android" qualifier="hdpi" />
  <splash src="res/screens/android/screen-xhdpi-portrait.png" platform="android" qualifier="xhdpi" />
  <splash src="res/screens/android/screen-xxhdpi-portrait.png" platform="android" qualifier="port-xxhdpi" />
  <splash src="res/screens/android/screen-xxxhdpi-portrait.png" platform="android" qualifier="xxxhdpi" />


  <splash src="res/screens/ios/Default~iphone.png" platform="ios" width="320" height="480"/>
  <splash src="res/screens/ios/Default@2x~iphone.png" platform="ios" width="640" height="960"/>
  <splash src="res/screens/ios/Default-Portrait~ipad.png" platform="ios" width="768" height="1024"/>
  <splash src="res/screens/ios/Default-Portrait@2x~ipad.png" platform="ios" width="1536" height="2048"/>
  <splash src="res/screens/ios/Default-568h@2x~iphone.png" platform="ios" width="640" height="1136"/>
  <splash src="res/screens/ios/Default-667h.png" platform="ios" width="750" height="1334"/>
  <splash src="res/screens/ios/Default-736h.png" platform="ios" width="1242" height="2208"/>
  <splash src="res/screens/ios/Default-Landscape@2x~ipad.png" platform="ios" width="2208" height="1242" />
  <splash src="res/screens/ios/Default-Landscape~ipad.png" platform="ios" width="1024" height="768" />
  <splash src="res/screens/ios/Default-Landscape@2x~ipad.png" platform="ios" width="2048" height="1536" />


  <access origin="*"/>
  <allow-intent href="http://*"/>
  <allow-intent href="https://*"/>
  <allow-intent href="ws://*"/>
  <allow-intent href="wss://*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
  <platform name="windows"/>
  <plugin name="cordova-plugin-camera">
    <param name="CAMERA_USAGE_DESCRIPTION" value="We are using the Camera for something..." />
    <param name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="We are using the Photo Library for something..." />
  </plugin>
  <plugin name="cordova-plugin-device" source="npm" spec="~1.1.6"/>
  <plugin name="cordova-plugin-device-motion" source="npm" spec="~1.2.5"/>
  <plugin name="cordova-plugin-device-orientation" source="npm" spec="~1.0.7"/>
  <plugin name="cordova-plugin-dialogs" source="npm" spec="~1.3.3"/>
  <plugin name="cordova-plugin-file" source="npm" spec="~4.3.3"/>
  <plugin name="cordova-plugin-uniquedeviceid" source="npm"/>
  <plugin name="cordova-plugin-file-transfer" source="npm" spec="~1.6.3"/>
  <plugin name="cordova-plugin-globalization" source="npm" spec="~1.0.3"/>
  <plugin name="cordova-plugin-inappbrowser" source="npm" spec="~1.6.1"/>
  <plugin name="cordova-plugin-themeablebrowser" source="npm" spec="~0.2.17"/>
  <plugin name="cordova-plugin-media" source="npm" spec="~3.0.1"/>
  <plugin name="cordova-plugin-network-information" source="npm" spec="~1.2.0"/>
  <plugin name="cordova-plugin-statusbar" source="npm" spec="~2.1.2"/>
  <plugin name="cordova-plugin-safariviewcontroller" spec="https://github.com/EddyVerbruggen/cordova-plugin-safariviewcontroller.git"/>
  <plugin name="phonegap-plugin-push" spec="1.8.0">
    <param name="SENDER_ID" value="XXXXXXX"/>
  </plugin>
  <plugin name="cordova-plugin-x-socialsharing" spec="https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin.git"/>
  <plugin name="phonegap-plugin-barcodescanner" spec="~6.0.6"/>

</widget>